# mic mute
"pactl set-source-mute 0 toggle"
	XF86AudioMicMute

# display settings
"arandr"
	XF86Display

# power settings
"xfce4-power-manager-settings"
	XF86Tools

# file manager
"thunar"
	XF86Search

# mail client
"thunderbird"
	XF86LaunchA

# screen lock
"i3lock -i ~/Pictures/ss.png -t"
	XF86Messenger
#	XF86Explorer

# print screen
"xfce4-screenshooter -f"
	Mod4 + Print
